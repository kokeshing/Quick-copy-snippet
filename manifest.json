{
  "manifest_version": 2,
  "name": "Quick Copy Snippet",
  "short_name": "QuickCopySnippet",
  "description": "Display copy button on snippets of website",
  "version": "0.2",
  "icons": {
    "64": "icon.png"
  },
  "content_scripts": [{
    "matches": [
      "http://*/*",
      "https://*/*"
    ],
    "js": [
      "content_scripts/main.js"
    ],
    "css": [
      "content_scripts/main.css"
    ]
  }],
  "permissions": [
    "tabs"
  ]
}